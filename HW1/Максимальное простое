def primenumb(n):
    for i in range(2, n):
        if n % i == 0:
            return 0
    return 1
    
print('Введите целое число')
number = int(input())
if number <= 2:
    print('Введите число больше 2')
number -= 1
while number >= 2:
    if primenumb(number) == 1:
        print('Максимальное целое, которое меньше заданного =', number)
        break
    else:
        number -= 1
