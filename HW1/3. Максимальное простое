x = int(input('Введите целое число'))
if x <= 2:
    print('Введите число больше 2')
x -= 1

def primenumb(n):
    for i in range(2, n):
        if n % i == 0:
            return 0
    return 1

while x >= 2:
    if primenumb(x) == 1:
        print('Максимальное целое, которое меньше заданного =', x)
        break
    else:
        x -= 1
